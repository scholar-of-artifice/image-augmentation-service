{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "json_formatter": {
            "format": "%(message)s"
        }
    },
    "handlers": {
        "stdout": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "json_formatter",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
              "stdout"
            ]
        }
    }
}

